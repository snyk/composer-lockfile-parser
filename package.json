{
  "name": "@snyk/composer-lockfile-parser",
  "description": "Generate a dep tree given a composer.lock file",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint && npm run unit-test",
    "unit-test": "tap -Rspec ./test/*.test.ts --timeout=300",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/composer-lockfile-parser.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "dist"
  ],
  "keywords": [
    "snyk",
    "php",
    "composer",
    "parser"
  ],
  "engines": {
    "node": ">=6"
  },
  "homepage": "https://github.com/snyk/composer-lockfile-parser#readme",
  "author": "snyk.io",
  "license": "Apache-2.0",
  "dependencies": {
    "@snyk/lodash": "^4.17.15-patch"
  },
  "devDependencies": {
    "@types/node": "6.14.6",
    "@types/node-fetch": "^2.5.2",
    "node-fetch": "^2.6.0",
    "tap": "12.7.0",
    "tslint": "5.17.0",
    "typescript": "3.7.3"
  }
}
